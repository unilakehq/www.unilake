<div id="overlay" class="invisible transition-transform duration-500 ease-in-out"></div>
<div id="sidebar-overlay" class="invisible transition-transform duration-500 ease-in-out lg:hidden"></div>

<script>
    document.addEventListener('DOMContentLoaded', function () {
        const mobileMenu = document.getElementById('mobile-menu');
        const overlay = document.getElementById('overlay');
        const sidebarOverlay = document.getElementById('sidebar-overlay');
        const mobileMenuToggle = document.querySelector('button[type="button"]');
        const closeMenuButton = document.getElementById('close-side-bar');
        const dropdowns = document.querySelectorAll('#dropdownDefaultButton');

        let lastScrollTop = 0;
        const header = document.getElementById("main-header");
        window.addEventListener("scroll", function () {
            const currentScroll = window.pageYOffset || document.documentElement.scrollTop;
            if (currentScroll > lastScrollTop) {
                header.classList.add("hidden-header");
                overlay.classList.add("hidden-overlay");
            } else {
                header.classList.remove("hidden-header");
                overlay.classList.remove("hidden-overlay");
            }
            lastScrollTop = currentScroll <= 96 ? 96 : currentScroll;
        }, false);
        mobileMenuToggle.addEventListener('click', () => {
            mobileMenu.classList.toggle('translate-x-0');
            mobileMenu.classList.toggle('translate-x-full');
            displaySidbarOverlay();
        });

        closeMenuButton.addEventListener('click', function () {
            mobileMenu.classList.toggle('translate-x-0');
            mobileMenu.classList.toggle('translate-x-full');
            hideSidbarOverlay();
        });

        dropdowns.forEach(dropdown => {
            dropdown.addEventListener('focus', function (event) {
                displayOverlay()
            });
            dropdown.addEventListener('blur', function (event) {
                overlay.classList.toggle("invisible");
            });
        });


        function displayOverlay() {
            overlay.classList.add("block");
            overlay.classList.remove("invisible");
        }

        function hideOverlay() {
            overlay.classList.remove("block");
            overlay.classList.add("invisible");
        }

        function displaySidbarOverlay() {
            sidebarOverlay.classList.add("block");
            sidebarOverlay.classList.remove("invisible");
        }

        function hideSidbarOverlay() {
            sidebarOverlay.classList.remove("block");
            sidebarOverlay.classList.add("invisible");
        }

        window.toggleSidebarMenu = function (accordion, event) {
            event.preventDefault();
            var content = accordion.nextElementSibling;
            var icon = accordion.querySelector('.accordion-icon');
            if (content.classList.contains('invisible') || content.classList.contains('max-h-0')) {
                icon.classList.add('-rotate-180');
                const classesToAdd = ['max-h-full', 'mt-S', 'md:mt-L'];
                const classesToRemove = ['invisible', 'max-h-0'];
                classesToRemove.forEach(className => {
                    content.classList.remove(className);
                });
                classesToAdd.forEach(className => {
                    content.classList.add(className);
                });
            } else {
                icon.classList.remove('-rotate-180');
                const classesToAdd = ['invisible', 'max-h-0'];
                const classesToRemove = ['max-h-full', 'mt-S', 'md:mt-L'];
                classesToRemove.forEach(className => {
                    content.classList.remove(className);
                });
                classesToAdd.forEach(className => {
                    content.classList.add(className);
                });
            }
        }
    });

</script>