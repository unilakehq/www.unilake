<script>

    function showData(accordion, event) {
        var content = accordion.parentNode.nextElementSibling;
        var icon = event.currentTarget.children[0]
        if (content.classList.contains('invisible') || content.classList.contains('max-h-0')) {
            icon.classList.add('-rotate-180');
            const classesToAdd = ['max-h-full', 'mt-S', 'md:mt-L', 'duration-1000'];
            const classesToRemove = ['invisible', 'max-h-0'];
            if (content.classList.contains('duration-500')) { classesToRemove.push('duration-500') }
            classesToRemove.forEach(className => {
                content.classList.remove(className);
            });
            classesToAdd.forEach(className => {
                content.classList.add(className);
            });
        } else {
            icon.classList.remove('-rotate-180');
            const classesToAdd = ['invisible', 'max-h-0', 'duration-500'];
            const classesToRemove = ['max-h-full', 'mt-S', 'md:mt-L', 'duration-1000'];
            classesToRemove.forEach(className => {
                content.classList.remove(className);
            });
            classesToAdd.forEach(className => {
                content.classList.add(className);
            });
        }
    }
    function toggleTag(event) {
        const currentTag = event.currentTarget;
        if (currentTag.classList.contains('active-tag')) {
            currentTag.classList.remove('active-tag');
            currentTag.classList.add('inactive-tag');
        } else {
            currentTag.classList.remove('inactive-tag');
            currentTag.classList.add('active-tag');
        }
    }
